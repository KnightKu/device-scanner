{
  "projFile": "LineDelimitedJsonStream.fsproj",
  "sourceMaps": true,
  "outDir": "dist",
  "module": "es2015",
  "ecma": "es2015",
  "rollup": {
    "plugins": [
      [
        "babel",
        {
          "presets": [
            [
              "env",
              {
                "targets": {
                  "node": "current"
                },
                "modules": false
              }
            ]
          ],
          "babelrc": false
        }
      ],
      [
        "node-resolve",
        {
          "main": true
        }
      ],
      [
        "cleanup",
        {
          "maxEmptyLines": 0
        }
      ]
    ]
  }
}
